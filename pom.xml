  <build>
    <resources>
      <resource>
        <directory>src/main/java</directory>
        <includes>
          <include>**/*.java</include>
          <include>**/*.class</include>
          <include>**/*.gwt.xml</include>
          <include>**/*.png</include>
          <include>**/*.jpeg</include>
          <include>**/*.jpg</include>
        </includes>
      </resource>
      <resource>
        <directory>src/main/resources</directory>
        <includes>
          <include>**/*.java</include>
          <include>**/*.gwt.xml</include>
          <include>**/*.png</include>
          <include>**/*.jpeg</include>
          <include>**/*.jpg</include>
          <include>**/*.js</include>
          <include>**/*.html</include>
          <include>**/*.xhtml</include>
          <include>**/*.xml</include>
          <include>**/*.jsp</include>
          <include>**/*.css</include>
        </includes>
      </resource>
      <resource>
        <directory>src/main/webapp</directory>
        <includes>
          <include>**/*</include>
        </includes>
      </resource>
    </resources>
<!-- 		Strelock : define necessary of this thing?! -->
    <extensions>
