#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="${appClass}">
	<inherits name="tripleplay.TriplePlay" />
	<inherits name="ApplicationAssets" />
	<inherits name="com.toxic.CoreApplication" />

	<source path="core" />
	<source path="html" />

	<public path="resources" />

	<entry-point class="${package}.html.${appClass}Html" />
  
     <!-- allow Super Dev Mode -->
    <add-linker name="xsiframe" />
  
</module>
